import java.util.*;
public class Quick{
    private static int partition(int[] data,int left,int right){
	int rnd = new Random().nextInt(data.length);
	int temp=0;
	temp = data[rnd];
	data[rnd]=data[right];
	data[right]=temp;
	right--;
	int num=0;
	while(right>=left){
	    if(data[left]>=temp){;
		right--;}
	    if(data[right]<=temp){
		left++;}
  
	}

	return right;
    }

    public static void main(String[]args){
	Quick q = new Quick();
	int[] data = {2,3,5,6,3,4,7,8};
	System.out.println(q.partition(data,0,7));
    }
}
